//AntiJetScaleFactors(Taus): 2018
double VLLAna::getScaleFactors_Taus_AntiJet(float pt){
  double scale_factor=1.0;

  if( 20<pt && pt<25)
    scale_factor = ;
  if( 25<pt && pt<30)
    scale_factor = ;
  if( 30<pt && pt<35)
    scale_factor = ;
  if( 35<pt && pt<40)
    scale_factor = ;
  if( 40<pt && pt<500)
    scale_factor = ;
  if( 500<pt && pt<1000)
    scale_factor = ;
  if( 1000<pt && pt<2000)
    scale_factor = ;
  
  return scale_factor;

}
//AntiEleScaleFactors(Taus): 2018
double VLLAna::getScaleFactors_Taus_AntiEle(float eta){
  double scale_factor=1.0;

  if( 0.0<eta && eta<1.46)
    scale_factor = ;
  if( 1.46<eta && eta<1.558)
    scale_factor = ;
  if( 1.558<eta && eta<2.3)
    scale_factor = ;
    
  return scale_factor;
}

//AntiMuScaleFactors(Taus): 2018
double VLLAna::getScaleFactors_Taus_AntiMu(float eta){
  double scale_factor=1.0;

  if( 0.0<eta && eta<0.4)
    scale_factor = ;
  if( 0.4<eta && eta<0.8)
    scale_factor = ;
  if( 0.8<eta && eta<1.2)
    scale_factor = ;
  if( 1.2<eta && eta<1.7)
    scale_factor = ;
  if( 1.7<eta && eta<2.3)
    scale_factor = ;
  
  return scale_factor;

}
